{
    "name": "uraankhayayaal/yii2-page",
    "description": "Page module for Yii2 with slug",
    "type": "yii2-extension",
    "keywords": ["yii2"," extension"," page"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ayaal Kaplin",
            "email": "uraankhayayaal@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "uraankhayayaal/yii2-sortable": "*",
        "uraankhayayaal/yii2-materializecomponents": "*",
        "uraankhayayaal/yii2-redactor": "*",
        "kartik-v/yii2-widget-colorinput": "*",
        "deryabinsergey/yii2-chartjs": "*",
        "yiisoft/yii2-bootstrap4": "*"
    },
    "autoload": {
        "psr-4": {
            "uraankhayayaal\\page\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
